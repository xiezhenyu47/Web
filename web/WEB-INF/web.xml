<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <!--//登陆用的servelet-->
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>BaseFunction.login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <!--//注册用的servelet-->
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>BaseFunction.register</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <!--//人脸识别用的servelet-->
    <servlet>
        <servlet-name>Photocmp</servlet-name>
        <servlet-class>FaceCompare.Photocmp</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Photocmp</servlet-name>
        <url-pattern>/Photocmp</url-pattern>
    </servlet-mapping>
    <!--//举行会议用的servelet-->
    <servlet>
        <servlet-name>company</servlet-name>
        <servlet-class>BaseFunction.company</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>company</servlet-name>
        <url-pattern>/company</url-pattern>
    </servlet-mapping>
</web-app>